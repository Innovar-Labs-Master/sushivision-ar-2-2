# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Gemini AI API Key (vereist voor AI features)
# Haal je API key op: https://aistudio.google.com/apkey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Alternative (voor server-side als nodig)
GEMINI_API_KEY=your_gemini_api_key_here

# Server-side Security
# JWT Secret for authentication tokens (CHANGE THIS IN PRODUCTION!)
JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters-long-please-change-in-production

# Node Environment
NODE_ENV=development

# Hugging Face API Token (optioneel, voor model finder scripts)
HF_API_TOKEN=your_huggingface_token_here
